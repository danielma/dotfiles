;; base16-<%- schemeSlug %>-light.el -- A base16 colorscheme

;;; Commentary:
;; Base16: (https://github.com/chriskempson/base16)

;;; Code:

(require 'base16-theme)

(defvar base16-<%- schemeSlug %>-light-colors
  '(:base00 "#<%- base["07"]["hex"] %>"
    :base01 "#<%- base["06"]["hex"] %>"
    :base02 "#<%- base["05"]["hex"] %>"
    :base03 "#<%- base["04"]["hex"] %>"
    :base04 "#<%- base["03"]["hex"] %>"
    :base05 "#<%- base["02"]["hex"] %>"
    :base06 "#<%- base["01"]["hex"] %>"
    :base07 "#<%- base["00"]["hex"] %>"
    :base08 "#<%- base["08"]["hex"] %>"
    :base09 "#<%- base["09"]["hex"] %>"
    :base0A "#<%- base["0A"]["hex"] %>"
    :base0B "#<%- base["0B"]["hex"] %>"
    :base0C "#<%- base["0C"]["hex"] %>"
    :base0D "#<%- base["0D"]["hex"] %>"
    :base0E "#<%- base["0E"]["hex"] %>"
    :base0F "#<%- base["0F"]["hex"] %>")
  "All colors for Base16 <%- schemeSlug %> are defined here.")

;; Define the theme
(deftheme base16-<%- schemeSlug %>-light)

;; Add all the faces to the theme
(base16-theme-define 'base16-<%- schemeSlug %>-light base16-<%- schemeSlug %>-light-colors)

;; Mark the theme as provided
(provide-theme 'base16-<%- schemeSlug %>-light)

(provide 'base16-<%- schemeSlug %>-light-theme)

;;; base16-gruvbox-theme.el ends here