"*":
  editor:
    showIndentGuide: true
    fontFamily: "Input Mono Narrow"
    invisibles: {}
    softWrapAtPreferredLineLength: true
    fontSize: 12
    lineHeight: 1.1
  core:
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    hideGitIgnoredFiles: true
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
    ]
    themes: [
      "atom-dark-ui"
      "base16-tomorrow-dark-theme"
    ]
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  welcome:
    showOnStartup: false
  "release-notes":
    viewedVersion: "0.89.0"
  "linter-rubocop":
    command: "/Users/danielma/.rbenv/shims/rubocop"
