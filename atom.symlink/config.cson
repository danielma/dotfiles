"*":
  "atom-package-deps":
    ignored: []
  "atom-typescript": {}
  core:
    disabledPackages: [
      "background-tips"
      "exception-reporting"
      "metrics"
      "react"
      "language-gfm"
    ]
    hideGitIgnoredFiles: true
    ignoredNames: [
      ".bundle"
      ".git"
      "log"
      "repositories"
      "tmp"
      "vendor"
    ]
    telemetryConsent: "limited"
    titleBar: "custom-inset"
  editor:
    fontFamily: "SFMono-Regular"
    fontSize: 12
    invisibles: {}
    lineHeight: 1.2
    preferredLineLength: 100
    scrollPastEnd: true
    showIndentGuide: true
    showInvisibles: true
    softWrapAtPreferredLineLength: true
  "ide-flow":
    flowPath: "/usr/local/bin/flow"
  linter: {}
  "linter-eslint":
    fixOnSave: true
  "linter-rubocop":
    command: "/Users/danielma/.rbenv/shims/rubocop"
  "linter-ui-default":
    showPanel: true
  "markdown-writer": {}
  pigments: {}
  "release-notes":
    viewedVersion: "0.89.0"
  "spell-check":
    grammars: [
      "text.plain"
      "source.gfm"
      "text.git-commit"
    ]
  "split-diff":
    ignoreWhitespace: true
  "vim-mode": {}
  welcome:
    showOnStartup: false
